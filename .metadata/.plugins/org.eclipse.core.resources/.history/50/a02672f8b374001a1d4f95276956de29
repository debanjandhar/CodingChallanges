package hackerrank;

import java.util.Stack;

public class JimAndSkyscrapers {

	static int solve(int[] arr) {
		Stack<Integer> stack = new Stack<Integer>();
		int count = 0;
		for (int i : arr) {
			if (stack.isEmpty()) {
				stack.push(i);
			}else if (stack.peek() == ) {
				
			}
		}
		
		return count;
	}

	public static void main(String[] args) {
		int[] arr = new int[] { 1, 2 };

		System.out.println(solve(arr));
	}
}
